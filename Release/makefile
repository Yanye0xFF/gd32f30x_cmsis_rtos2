# Add inputs and outputs from these tool invocations to the build variables 
SECONDARY_FLASH += \
gd32f30x_cmsis_rtos2.bin \

SECONDARY_SIZE += \
gd32f30x_cmsis_rtos2.siz \


# All Target
all: main-build

# Main-build Target
main-build: gd32f30x_cmsis_rtos2.elf secondary-outputs

# Tool invocations
gd32f30x_cmsis_rtos2.elf: $(OBJS) $(USER_OBJS) makefile objects.mk $(OPTIONAL_TOOL_DEPS)
	@echo 'Building target: $@'
	@echo 'Invoking: GNU Arm Cross C++ Linker'
	arm-none-eabi-g++ -mcpu=cortex-m4 -march=armv7e-m -mthumb -mlittle-endian -mfloat-abi=hard -mfpu=fpv4-sp-d16 -mno-unaligned-access -O2 -fmessage-length=0 -fsigned-char -ffunction-sections -fdata-sections -fno-common -ffreestanding -Wuninitialized -Wall -Wextra -Wshadow -Waggregate-return -Wfloat-equal -T gd32f30x_flash.ld -nostartfiles -Xlinker --gc-sections -L"../src/ldscript" -Wl,-Map,"gd32f30x_cmsis_rtos2.map" --specs=nano.specs -o "gd32f30x_cmsis_rtos2.elf" $(OBJS) $(USER_OBJS) $(LIBS)
	@echo 'Finished building target: $@'
	@echo ' '

gd32f30x_cmsis_rtos2.bin: gd32f30x_cmsis_rtos2.elf makefile objects.mk $(OPTIONAL_TOOL_DEPS)
	@echo 'Invoking: GNU Arm Cross Create Flash Image'
	arm-none-eabi-objcopy -O binary "gd32f30x_cmsis_rtos2.elf"  "gd32f30x_cmsis_rtos2.bin"
	@echo 'Finished building: $@'
	@echo ' '

gd32f30x_cmsis_rtos2.siz: gd32f30x_cmsis_rtos2.elf makefile objects.mk $(OPTIONAL_TOOL_DEPS)
	@echo 'Invoking: GNU Arm Cross Print Size'
	arm-none-eabi-size --format=berkeley "gd32f30x_cmsis_rtos2.elf"
	@echo 'Finished building: $@'
	@echo ' '

# Other Targets
clean:
	-$(RM) $(CC_DEPS)$(C++_DEPS)$(OBJS)$(C_UPPER_DEPS)$(CXX_DEPS)$(SECONDARY_FLASH)$(SECONDARY_SIZE)$(ASM_DEPS)$(S_UPPER_DEPS)$(C_DEPS)$(CPP_DEPS) gd32f30x_cmsis_rtos2.elf
	-@echo ' '

secondary-outputs: $(SECONDARY_FLASH) $(SECONDARY_SIZE)

